{
    "tables": [
        {
            "name": "yuanshop_shop_goods",
            "model": "product.template",
            "method": "product_import",
            "seq": "110",
            "filter": "goodssn is not null and goodssn!=''",
            "columns": {
                "id": {
                    "ctype": "int",
                    "alias": "id"
                },
                "title": {
                    "ctype": "string",
                    "alias": "name"
                },
                "goodssn": {
                    "ctype": "string",
                    "alias": "default_code"
                },
                "marketprice": {
                    "ctype": "string",
                    "alias": "list_price"
                },
                "thumb": {
                    "ctype": "image",
                    "alias": "image_url"
                },
                "ccate":{
                    "ctype":"string",
                    "alias":"third_cate_id"
                }
            },
            "children": [
                {
                    "name": "yuanshop_shop_goods_spec",
                    "alias": "spec_info",
                    "filter": "1=2",
                    "relcol": "goodsid",
                    "columns": {
                        "id": {
                            "ctype": "int"
                        },
                        "title": {
                            "ctype": "string"
                        }
                    }
                }
            ]
        },
        {
            "name": "yuanshop_shop_category",
            "model": "product.category",
            "method": "cate_import",
            "seq": "100",
       
            "columns": {
                "id": {
                    "ctype": "int",
                    "alias": "third_id"
                },
                "name": {
                    "ctype": "string",
                    "alias": "name"
                },
                "parentid": {
                    "ctype": "string",
                    "alias": "parent_id"
                }
            }
        }
    ]
}