{
    "tables": [
        {
            "name": "yuanshop_shop_goods",
            "filter": "goodssn is not null and goodssn!=''",
            "columns": {
                "id": {
                    "ctype": "int",
                    "alias": "id"
                },
                "title": {
                    "ctype": "string",
                    "alias": "name"
                },
                "goodssn": {
                    "ctype": "string",
                    "alias": "default_code"
                },
                "marketprice": {
                    "ctype": "string",
                    "alias": "list_price"
                },
                "thumb": {
                    "ctype": "image",
                    "alias": "image_url"
                }
            },
            "children": [
                {
                    "name": "yuanshop_shop_goods_spec",
                    "alias": "spec_info",
                    "filter": "1=2",
                    "relcol": "goodsid",
                    "columns": {
                        "id": {
                            "ctype": "int"
                        },
                        "title": {
                            "ctype": "string"
                        }
                    }
                }
            ]
        }
    ]
}